{
    "presets": [
        ["@babel/preset-env", { 
            "modules": false,
            "useBuiltIns": "usage"
        }],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }] 
    ]
}